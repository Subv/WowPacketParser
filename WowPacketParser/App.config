<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <appSettings>
        <!-- Parsing settings -->
        <add key="ClientBuild" value=""/>
        <add key="Filters" value=""/>
        <add key="IgnoreFilters" value=""/>
        <add key="SQLOutput" value="true"/>
        <add key="Threads" value="0"/>
        <!-- DumpFormat:
            0: No dump
            1: Text
            2: .bin
            3: .pkt
            4: Text: Only the headers of each packet
            5: Summary Text: Only the header of first occurrence of each Opcode (match by Opcode name and direction)
        -->
        <add key="DumpFormat" value="1"/>
        <add key="PacketsNum" value="0"/>
        <add key="FilterPacketNumLow" value="0"/>
        <add key="FilterPacketNumHigh" value="0"/>
        <add key="ShowEndPrompt" value="false"/>
        <!-- DBC settings -->
        <add key="DBCEnabled" value="true"/>
        <add key="DBCPath" value="./"/>
        <!-- MySQL connection settings -->
        <add key="DBEnabled" value="false" />
        <add key="Server" value="localhost" />
        <add key="Port" value="3306" />
        <add key="Username" value="root" />
        <add key="Password" value="" />
        <add key="Database" value="world" />
        <add key="CharacterSet" value="utf8" />
    </appSettings>
</configuration>
